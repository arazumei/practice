#include <stdio.h>

int main() {
    int t1, t2, t3;
    float total_time;
    
    printf("Введіть три значення:");
    // Зчитуємо значення t1, t2, t3 зі вхідного рядка
    scanf("%d %d %d", &t1, &t2, &t3);

    // Обчислюємо загальний час
    total_time = (float)(1 / (1.0/t1 + 1.0/t2 + 1.0/t3));

    // Виводимо результат з округленням до 2 знаків після коми
    printf("Час, необхідний для з'їдання пирога: %.2f годин\n", total_time);

    return 0;
}
